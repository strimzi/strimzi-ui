{
  "name": "strimzi-ui",
  "private": true,
  "version": "0.0.1",
  "description": "Strimzi UI provides a way for managing Strimzi and Kafka clusters (+ other components) deployed by it using a graphical user interface.",
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf generated/",
    "start": "webpack-dev-server --config utils/build/webpack.dev.js",
    "build": "webpack --config utils/build/webpack.prod.js",
    "pretest": "npm-run-all --parallel licence:check && prettier --config ./utils/linting/prettier.config.js --check .",
    "test": "echo https://github.com/strimzi/strimzi-ui/issues/7",
    "licence:check": "license-check-and-add --f ./utils/linting/license-check-and-add.config.json check",
    "licence:add": "license-check-and-add --f ./utils/linting/license-check-and-add.config.json add",
    "lint": "npm-run-all 'lint:allfiles -- .' 'lint:js -- .' 'lint:styling -- \"**/*.(s)css\"' 'lint:format -- .'",
    "lint:allfiles": "npm-run-all 'licence:add -- {1}' --",
    "lint:js": "eslint --config ./utils/linting/eslint.config.js --fix --max-warnings 0",
    "lint:styling": "stylelint --config ./utils/linting/stylelint.config.js --fix --max-warnings 0",
    "lint:format": "prettier --config ./utils/linting/prettier.config.js --write"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strimzi/strimzi-ui.git"
  },
  "license": "Apache-2.0",
  "bugs": {},
  "homepage": "https://github.com/strimzi/strimzi-ui#readme",
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass": "^1.26.10",
    "terser-webpack-plugin": "^3.0.7",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "commitlint": "^9.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "license-check-and-add": "^3.0.4",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
